{
  "name": "@z0mt3c/f1-telemetry-client",
  "version": "2.0.0-beta.15",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    },
    "./constants": {
      "import": "./dist/esm/constants/index.js",
      "require": "./dist/cjs/constants/index.js",
      "types": "./dist/esm/constants/index.d.ts"
    },
    "./types": {
      "import": "./dist/esm/types/index.js",
      "require": "./dist/cjs/types/index.js",
      "types": "./dist/esm/types/index.d.ts"
    },
    "./types/contentTypes": {
      "import": "./dist/esm/types/contentTypes.js",
      "require": "./dist/cjs/types/contentTypes.js",
      "types": "./dist/esm/types/contentTypes.d.ts"
    },
    "./types/parserTypes": {
      "import": "./dist/esm/types/parserTypes.js",
      "require": "./dist/cjs/types/parserTypes.js",
      "types": "./dist/esm/types/parserTypes.d.ts"
    }
  },
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/z0mt3c/f1-telemetry-client.git"
  },
  "dependencies": {
    "binary-parser": "^2.2.1",
    "struct": "^0.0.12"
  },
  "devDependencies": {
    "@types/binary-parser": "^1.5.5",
